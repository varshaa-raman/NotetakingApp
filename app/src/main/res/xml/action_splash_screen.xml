<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:autoTransition="animateToEnd">
        <KeyFrameSet>

            <KeyAttribute
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="10"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="0.5"
                android:scaleY="0.5"
                motion:framePosition="20"
                motion:motionTarget="@+id/iv_spash_image" />

            <KeyAttribute
                android:scaleX="0.5"
                android:scaleY="0.5"
                motion:framePosition="30"
                motion:motionTarget="@+id/iv_spash_image" />

            <KeyAttribute
                android:scaleX="0.5"
                android:scaleY="0.5"
                motion:framePosition="40"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="0.5"
                android:scaleY="0.5"
                motion:framePosition="50"
                motion:motionTarget="@+id/iv_spash_image" />

            <KeyAttribute
                android:scaleX="1.5"
                android:scaleY="1.5"
                motion:framePosition="60"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="2.5"
                android:scaleY="2.5"
                motion:framePosition="70"
                motion:motionTarget="@+id/iv_spash_image" />

            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="100"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="120"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="150"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="170"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="190"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="200"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="220"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="240"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="260"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="280"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="300"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="320"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="340"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="360"
                motion:motionTarget="@+id/iv_spash_image" />
            <KeyAttribute
                android:scaleX="25"
                android:scaleY="25"
                motion:framePosition="380"
            motion:motionTarget="@+id/iv_spash_image" />


        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/iv_spash_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

    </ConstraintSet>
</MotionScene>